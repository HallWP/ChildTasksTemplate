{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
         {
            "name": "Launch Firefox",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "url": "https://localhost:3000",
            "webRoot": "${workspaceFolder}",
            "pathMappings": [
                { "url": "webpack:///~/", "path": "${webRoot}/node_modules/" },
                { "url": "webpack:///./~/", "path": "${webRoot}/node_modules/" },
                { "url": "webpack:///./", "path": "${webRoot}/" },
                { "url": "webpack:///src/", "path": "${webRoot}/src/" },
                { "url": "webpack:///node_modules/", "path": "${webRoot}/node_modules/" },
                { "url": "webpack:///webpack", "path": null },
                { "url": "webpack:///(webpack)", "path": null },
                { "url": "webpack://[name]_[chunkhash]/node_modules/", "path": "${webRoot}/node_modules/" },
                { "url": "webpack://[name]_[chunkhash]/", "path": null },
                { "url": "webpack:///", "path": "" },
                {
                    "url": "webpack:///home/vsts/work/1/s/src/app.ts",
                    "path": "${workspaceFolder}/src/app.ts"
                },
                {
                    "url": "webpack:///src/app.ts",
                    "path": "${workspaceFolder}/src/app.ts"
                },
                {
                    "url": "webpack:///src/ChildTaskService.ts",
                    "path": "${workspaceFolder}/src/ChildTaskService.ts"
                }
            ]
        }
    ]
}